+                if ($object->user_can_remove() && $object->delete()) {
+                    $success = true;
+                }
+            } else {
+                $success = true;
+            }
+
+        }
+        if ($success) {
+            echo json_encode('success');
+        } else {
+            throw new moodle_exception("failed to delete $class $id");
+        }
+    break;
+}
