+            $data->objectid = $object->objectid;
+            echo json_encode($data);
+        } else if ($action == 'metainfo') {
+            $info = $object->get_structured_metainfo();
+            echo json_encode($info);
+        }
+    break;
+
+    case 'POST':
+        if ($action == 'sortorder') {
+            $object->update_sortorder($data);
+        } else if ($action == 'like' || $action == 'unlike') {
+            $count = $object->$action();
