+    public function get_source() {
+        if (empty($this->record->externalurl) && empty($this->record->objectid)) {
+            return 'internal';
+        }
+        if ($this->get_youtube_videoid()) {
+            return 'youtube';
+        }
+        return 'external';
+    }
