+    }
+
+    /**
+     * Validate form input.
+     *
+     * @param array $data
+     * @param array $files
+     * @return array List of errors, if any.
+     */
+    public function validation($data, $files) {
+        $errors = array();
+        $collection = new \mod_mediagallery\collection($data['m']);
+        return $errors;
+    }
+}
