+                </div>
+
+                <div class="options">
+                    <div class="instruction">
+                        {{instructions}}
+                    </div>
+                    <div class="alloptions">
+                        {{#sections}}
+                            <div class="modchooser_{{id}} modchoosersection">
+                                <div class="moduletypetitle">
+                                    <label for="item_{{id}}">
+                                        <span class="typename">{{label}}</span>
+                                    </label>
+                                </div>
+                                {{#items}}
+                                    {{>core/chooser_item}}
