+	for (var i =0; i < tabs.length; i++)
+		if (tabs[i] == tab)
+		{
+			var el = false;
+			if (key == this.previousPanelKeyCode && i > 0)
+				el = tabs[i-1];
+			else if (key == this.nextPanelKeyCode && i < tabs.length-1)
+				el = tabs[i+1];
+
+			if (el)
+			{
+				this.showPanel(el);
+				el.focus();
+				break;
+			}
+		}
