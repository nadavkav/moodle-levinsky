+        }
+
+        if (empty($section)) {
+            $section = 1;
+        }
+
+        if (isset($this->questionsbysec)) {
+            $numsections = count($this->questionsbysec);
+        } else {
+            $numsections = 0;
+        }
+
+        if ($section > $numsections) {
+            return(false);  // Invalid section.
+        }
+
+        $hasrequired = $this->has_required();
+
+        // Find out what question number we are on $i.
+        $i = 1;
+        for ($j = 2; $j <= $section; $j++) {
