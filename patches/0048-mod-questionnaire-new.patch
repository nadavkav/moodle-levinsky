+
+        $descendantsandchoices = array();
+
+        if ($referer == 'preview' && questionnaire_has_dependencies($this->questions) ) {
+                $descendantsandchoices = questionnaire_get_descendants_and_choices($this->questions);
+        }
+        if ($errors == 0) {
