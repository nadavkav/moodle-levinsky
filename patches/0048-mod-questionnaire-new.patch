+    protected function question_survey_display($data, $descendantsdata, $blankquestionnaire=false) {
+        // Check boxes.
+        $otherempty = false;
+        if (!empty($data) ) {
+            if (!isset($data->{'q'.$this->id}) || !is_array($data->{'q'.$this->id})) {
