+        }
+
+        /* delete values */
+        $select = 'response_id = \'' . $rid . '\' ' . $qsql;
+        foreach (array('response_bool', 'resp_single', 'resp_multiple', 'response_rank', 'response_text',
+                       'response_other', 'response_date') as $tbl) {
