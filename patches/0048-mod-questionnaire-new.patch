+    private function has_required($section = 0) {
+        if (empty($this->questions)) {
+            return false;
+        } else if ($section <= 0) {
+            foreach ($this->questions as $question) {
+                if ($question->required == 'y') {
+                    return true;
+                }
+            }
+        } else {
+            foreach ($this->questionsbysec[$section] as $question) {
+                if ($question->required == 'y') {
+                    return true;
+                }
+            }
+        }
+        return false;
+    }
