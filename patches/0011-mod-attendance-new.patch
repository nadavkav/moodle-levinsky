+        echo $OUTPUT->notification(get_string('eventstatusupdated', 'attendance'), 'success');
+
+        break;
+}
+
+$statuses = attendance_get_statuses(0, false);
+$prefdata = new attendance_default_statusset($statuses, $errors);
+echo $output->render($prefdata);
+
